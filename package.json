  "devDependencies": {
    "@types/node": "14.17.15",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-lazyload": "3.1.1",

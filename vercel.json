{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=1, stale-while-revalidate=299, stale-if-error=86400"
                }
            ]
        },
        {
            "source": "/(.*).(woff2?|png|mp4|jpe?g|svg)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}